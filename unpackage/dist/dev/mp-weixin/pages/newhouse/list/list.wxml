 wx:for-index="index" wx:key="tagID"><view class="{{['more-tag-item',postdatafront.tagID==item.tagID?'filter-cur':'']}}" data-type="tagID" data-id="{{item.tagID}}" data-event-opts="{{[['tap',[['moretagFn',['$event']]]]]}}" bindtap="__e">{{''+item.tagName+''}}</view></block></view><view class="moretag-title">开盘时间</view><view style="overflow:hidden;"><view class="{{['more-tag-item',postdatafront.openDate==''?'filter-cur':'']}}" data-type="openDate" data-id data-event-opts="{{[['tap',[['moretagFn',['$event']]]]]}}" bindtap="__e">不限</view><block wx:for="{{filterData.openDate}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="{{['more-tag-item',postdatafront.openDate==item.id?'filter-cur':'']}}" data-type="openDate" data-id="{{item.id}}" data-event-opts="{{[['tap',[['moretagFn',['$event']]]]]}}" bindtap="__e">{{''+item.otherName+''}}</view></block></view></scroll-view></view><view class="tab-btn"><view data-event-opts="{{[['tap',[['moreCancel',['$event']]]]]}}" bindtap="__e">重置</view><view data-event-opts="{{[['tap',[['moreConfirm',['$event']]]]]}}" bindtap="__e">确定</view></view></view></block><block wx:if="{{tabindex==4}}"><view class="sorttab"><view class="{{[postdata.sortID==''?'filter-cur':'']}}" data-sortid data-event-opts="{{[['tap',[['sortFn',['$event']]]]]}}" bindtap="__e">默认排序</view><block wx:for="{{filterData.order}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="{{[postdata.sortID==item.id?'filter-cur':'']}}" data-sortid="{{item.id}}" data-event-opts="{{[['tap',[['sortFn',['$event']]]]]}}" bindtap="__e">{{''+item.orderName+''}}</view></block></view></block></view></block><block wx:if="{{!loadstatus}}"><view class="loading"><image class="loading-img" src="../../../static/img/resources/load.png"></image></view></block></view>