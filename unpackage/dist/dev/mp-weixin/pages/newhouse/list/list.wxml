<view><view class="list-search"><view data-event-opts="{{[['tap',[['searchhouse',['$event']]]]]}}" class="list-search-box" bindtap="__e"><image src="../../../static/img/resources/newhouse/search.png"></image><text>输入小区或商圈名称</text></view></view><view class="list-tab"><block wx:for="{{tabArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['list-item',item.select?'filter-cur':tabindex==index?'filter-cur':'']}}" data-index="{{index}}" data-event-opts="{{[['tap',[['filtertabFn',['$event']]]]]}}" bindtap="__e"><block wx:if="{{item.name!='排序'}}"><view><text>{{item.name}}</text><text class="arrow"></text><view class="item-border"></view></view></block><block wx:else><view><image src="{{item.select?'../../../static/img/resources/newhouse/sort_select.png':tabindex==index?'../../../static/img/resources/newhouse/sort_select.png':'../../../static/img/resources/newhouse/sort.png'}}"></image></view></block></view></block></view><view class="list-data"><scroll-view scroll-y="true" lower-threshold="50" data-event-opts="{{[['scrolltolower',[['scrolltolower',['$event']]]]]}}" bindscrolltolower="__e"><block wx:for="{{listdata}}" wx:for-item="item" wx:for-index="index" wx:key="estateID"><view class="list-ctn" data-estateid="{{item.estateID}}" data-propertytypeid="{{item.propertyTypeID}}" data-event-opts="{{[['tap',[['routego',['$event']]]]]}}" bindtap="__e"><view class="image-wrap"><image lazy-load="true" src="{{item.coverImageName}}"></image></view><view class="content"><view class="item-1"><text>{{item.estateName}}</text><view class="tag">{{item.saleStatus_desc}}</view></view><view class="item-2"><text>{{item.propertyTypeName}}</text>·<text>{{item.estateAreaName}}</text>-<text>{{item.estateAddress}}</text></view><view class="item-3"><block wx:for="{{item.tagList}}" wx:for-item="itemson" wx:for-index="indexson" wx:key="indexson"><view class="item-tag">{{itemson}}</view></block></view><view class="item-4"><text>{{item.show_price}}</text><block wx:if="{{item.min