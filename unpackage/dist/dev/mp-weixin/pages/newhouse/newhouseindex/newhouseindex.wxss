
page{
  font-family: 'PingFang-Medium'
}
view{
  margin:0;
  padding:0;
}
.common-padding{
  padding:0 40rpx;
}
.newhouse-search{
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-top: 10rpx;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.search-box{
  height: 80rpx;
  /* width:578rpx; */
  width:100%;
  border-radius: 16rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  box-shadow: 0 5rpx 20rpx 0 rgba(0,0,0,0.1);
  font-size: 28rpx;
  color:#A8A9AB;
  letter-spacing: 1px;
}
.search-box image{
  width:34rpx;
  height:34rpx;
  margin-left:27rpx;
  margin-right: 20rpx;
}
.news{
  width:38rpx;
  height:38rpx;
}
.banner{
  margin-top: 36rpx;
  height:200rpx;
  box-sizing: border-box;
  /* background:red; */
}
.banner-none{
  padding:0 40rpx;
  height:100%;
}
.banner-none image{
  width:100%;
  height:100%;
}
.banner swiper,.banner .swiper-item{
  width:100%;
  height:100%;
}
.banner .swiper-item image{
  width:calc(100% - 80rpx);
  height:100%;
  margin:0 auto;
  border-radius: 16rpx;
}
.swiper-item{
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  text-align: center;
}
.nav-bar{
  padding:0 60rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin-top:24rpx;
  margin-bottom: 32rpx;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.nav-bar view{
  width:106rpx;
  text-align: center;
}
.nav-bar view image{
  width:106rpx;
  height:116rpx;
}
.nav-bar text{
  font-size: 24rpx;
  color:#292b33;
  margin-top: -17rpx;
  display: block;
}
.tips{
  width:calc(100% - 80rpx);
  height:100rpx;
  margin:0 auto;
  border-top:1px solid #ecebeb;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  padding:36rpx 0;
}
.tipe-image{
  width:78rpx;
  height:76rpx;
  margin-left:32rpx;
}
.tip-loop{
  height:76rpx;
  width:calc(100% - 160rpx);
  overflow: hidden;
  margin-left:18rpx;
}
.tip-loop swiper-item,.tip-loop swiper{
  width:100%;
  height:100%;
}
.tip-loop swiper-item{
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.tip-title-wrap{
  width:calc(100% - 100rpx);
  height:100%;
  color:#292b33;
}
.tip-title{
  height: 14px;
  font-size: 14px;
  line-height: 14px;
  margin: 11rpx 0 8rpx;
}
.tip-sub-title{
  height: 12px;
  font-size: 12px;
  line-height: 16px;
}
.tip-image{
  width:76rpx;
  height:76rpx;
}
.card{
  width:100%;
}
.card_list{
  width:100%;
  height:220rpx;
}
.scroll-view_H{
	white-space: nowrap;
	height:220rpx;
	display:-webkit-box;
	display:-webkit-flex;
	display:flex;
}
.card_wrap{
  width:375rpx;
  height:220rpx;
  display: inline-block;
  margin-right:2rpx;
	border-radius:16px;
	overflow: hidden;
  position: relative;
  /* background: red; */
}
.card_wrap image{
  width:100%;
  height:100%;
}
.card_wrap .guide-text{
  position: absolute;
  z-index: 10;
  bottom: 56rpx;
  width: 86%;
  height:90rpx;
  line-height: 50rpx;
  display: block;
  font-size: 30rpx;
  color: #fff;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  left: 50%;
  white-space: normal;
  margin-left: -43%;
}
.scroll-view_H .card_wrap:nth-child(1){
  margin-left:40rpx;
}
.recommend .title{
  font-size: 44rpx;
  color:#292b33;
  font-weight: 600;
  margin-top:30rpx;
  margin-bottom: 42rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.recommend .title view:nth-child(2){
  font-size: 24rpx;
  color:#7d7e82;
}
.recommend .title view:nth-child(2) image{
  width:18rpx;
  height:18rpx;
}
.list-item{
  overflow: hidden;
  padding-bottom: 54rpx;
}
.image-wrap{
  width:210rpx;
  height:166rpx;
  float:left;
}
.image-wrap image{
  width:100%;
  height:100%;
  border-radius: 16rpx;
}
.content{
  width:calc(100% - 230rpx);
  float:right;
}
.item-1{
  font-size: 32rpx;
  color:#292b33;
  /* margin-right: 16rpx; */
}
.item-1 text{
  display: inline-block;
  margin-right: 16rpx;
}
.tag{
  display: inline-block;
  font-size: 12px;
  color:#1c60ea; 
  border-radius: 17rpx;
  background:skyblue;
  padding:2rpx 8rpx;
}
.item-2{
  font-size: 22rpx;
  color:#7d7e82;
  padding:8rpx 0 0rpx;
  width:100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.item-tag{
  display: inline-block;
  padding:3px;
  background:#ebf5f7;
  color:#608997;
  font-size: 22rpx;
  border-radius: 17rpx;
  margin-right: 8rpx;
}
.item-4{
  margin-top: -5rpx;
}
.item-4 text:nth-child(1){
  font-size: 32rpx;
  color:#ff522f;
}
.item-4 text:nth-child(2){
  font-size: 22rpx;
  color:#7d7e82;
  margin-left:16rpx;
}
.setbg{
  width:100%;
  background:#f8f8f8;
}

